{
  "manifest_version": 3,
  "name": "URL Blocker",
  "version": "0.1",
  "permissions": [
    "scripting",
    "webRequest",
    "webRequestBlocking",
    "hostPermissions"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "background": {
    "service_worker": "background.js"
  }
}
